{
    "name": "RadicalResearch.Narrator.Website",
    "version": "0.0.0",
    "devDependencies": {
        "babel-cli": "^6.3.15",
        "babel-preset-es2015": "^6.3.13",
        "eslint": "^1.10.3",
        "eslint-config-standard": "^4.4.0",
        "eslint-plugin-standard": "^1.3.1",
        "gulp": "^3.9.0",
        "gulp-jspm": "^0.5.5",
        "gulp-sass": "^2.1.0",
        "gulp-shell": "^0.5.1",
        "jspm": "^0.16.15",
        "require-dir": "^0.3.0",
        "rimraf": "2.2.8"
    },
    "scripts": {
        "clean": "gulp clean"
    },
    "jspm": {
        "directories": {
            "baseURL": "wwwroot"
        },
        "devDependencies": {
            "babel": "npm:babel-core@^5.8.24",
            "babel-runtime": "npm:babel-runtime@^5.8.24",
            "core-js": "npm:core-js@^1.1.4"
        }
    },
    "babel": {
        "presets": [
            "es2015"
        ]
    },
    "eslintConfig": {
        "ecmaFeatures": {
            "forOf": true,
            "spread": true,
            "arrowFunctions": true,
            "classes": true,
            "blockBindings": true,
            "modules": true,
            "templateStrings": true
        },
        "env": {
            "browser": true
        },
        "rules": {
            "semi": 2
        }
    }
}
